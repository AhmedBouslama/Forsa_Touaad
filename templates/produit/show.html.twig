{% extends 'base.html.twig' %}

{% block title %}Produit{% endblock %}

{% block body %}
    <h1>Produit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ produit.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ produit.Name }}</td>
            </tr>
            <tr>
                <th>Brand</th>
                <td>{{ produit.Brand }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ produit.Description }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ produit.Etat }}</td>
            </tr>
            <tr>
                <th>Attribute</th>
                <td>{{ produit.Attribute }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ produit.Value }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_produit_index') }}">back to list</a>

    <a href="{{ path('app_produit_edit', {'id': produit.id}) }}">edit</a>

    {{ include('produit/_delete_form.html.twig') }}
{% endblock %}
